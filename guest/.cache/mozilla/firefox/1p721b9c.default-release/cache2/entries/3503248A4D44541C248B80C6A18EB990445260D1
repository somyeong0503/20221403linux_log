    <script type="text/javascript">
        function loadKcsWebFonts() {
            // const startDate = window.performance.now();
            // console.log("#start load font..");
            const contextPath = "";
            const fontList = [{
                fontFamily: "notoSans",
                fontWeight: "400",
                url: `${contextPath}/fonts/NotoSansKR-Regular.woff2`,
            }, {
                fontFamily: "notoSans",
                fontWeight: "500",
                url: `${contextPath}/fonts/NotoSansKR-Medium.woff2`,
            }, {
                fontFamily: "notoSans",
                fontWeight:"700",
                url: `${contextPath}/fonts/NotoSansKR-Bold.woff2`
            }, {
                fontFamily: "KakaoOTFBold",
                fontWeight: "700",
                url: `${contextPath}/fonts/KakaoOTFBold.woff2`
            }];

            fontList.forEach(function ({ fontFamily, fontWeight, url }) {
                loadFont(fontFamily, fontWeight, url);
            });

            // const endDate = window.performance.now();
            // console.log("#complete load font..", endDate - startDate + "ms");

            async function loadFont(fontFamily, fontWeight, url) {
                const font = new FontFace(fontFamily, `url(${url})`, {
                    style: "normal",
                    weight: fontWeight
                });
                await font.load();
                document.fonts.add(font);
            }
        }

        try {
            loadKcsWebFonts();
        }
        catch (e) {
            console.error("#error load font..");
        }
    </script>
